<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<VBox alignment="CENTER" minHeight="600.0" minWidth="600.0" style="-fx-background-color: #fff;"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Text fx:id="text" stroke="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER">
            <font>
                <Font name="Arial Bold" size="41.0"/>
            </font>
        </Text>
        <Button fx:id="main" mnemonicParsing="false"
                text="Back to home page" textAlignment="CENTER">
            <VBox.margin>
                <Insets top="100.0"/>
            </VBox.margin>
        </Button>
    </children>
</VBox>